<%= form_tag destroy_selected_users_path, :method => :delete do %>
<ul class="toolbar h-menu">
  <li><%= link_to 'New', new_user_path, :class => 'button' %></li>
  <li><%= submit_tag 'Delete', :id => 'delete-button', :disabled => true %></li>
</ul>

<ul class="list">
  <% @users.each do |u| %>
  <li>
    <section>
    <div class="select"><%= check_box_tag "user_ids[]", u.id %></div>
    <div class="title"><%= link_to "#{u.first_name} #{u.last_name}", u %></div>
    </section>
  </li>
  <% end %>
</ul>
<% end %>
