<%= form_tag destroy_selected_accounts_path, :method => :delete do %>
<ul class='toolbar h-menu'>
  <li><%= link_to 'New', new_account_category_path, :class => 'button' %></li>
  <li><%= submit_tag 'Delete', :id => 'delete-button', :disabled => true %></li>
</ul>

<ul class="list">
  <% for ac in @account_categories %>
  <li>
    <section>
      <div class="select"><%= check_box_tag "account_categories_ids[]", ac.id %></div>
      <div class="title"><%= link_to ac.name, ac %></div>
    </section>
  </li>
  <% end %>
</ul>
<% end %>
